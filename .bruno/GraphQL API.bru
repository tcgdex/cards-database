meta {
  name: GraphQL API
  type: graphql
  seq: 2
}

post {
  url: {{BASE_URL}}/v2/graphql
  body: graphql
  auth: none
}

body:graphql {
  query Pouet {
    cards {
      id
      localId
      name
      set {
        id
        name
        serie {
          id
          name
        }
      }
    }
  }
}

assert {
  res.status: eq 200
}
